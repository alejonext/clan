{
  "name": "clan",
  "version": "0.1.1",
  "description": "Servidor de Mochileros",
  "main": "bin/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel src -d lib",
    "start": "node lib",
    "postinstall": "npm cache clean",
    "dev": "nodemon --ignore 'public/*.js' -e js,pug --exec 'babel-node src'"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-member-expression-literals",
      "transform-export-extensions",
      "transform-merge-sibling-variables",
      "transform-node-env-inline"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alejonext/clan.git"
  },
  "keywords": [
    "server",
    "api",
    "clan",
    "digital"
  ],
  "author": "AlejoNext <alejonext@gmail.com> (http://alejonext.co/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/alejonext/clan/issues"
  },
  "homepage": "https://github.com/alejonext/clan#README",
  "dependencies": {
    "babel-plugin-transform-class-properties" : "^6.9.0",
    "babel-plugin-transform-member-expression-literals" : "^6.8.0",
    "babel-plugin-transform-export-extensions" : "^6.8.0",
    "babel-plugin-transform-merge-sibling-variables" : "^6.8.0",
    "babel-plugin-transform-node-env-inline": "^6.8.0",
    "angular": "^1.5.5",
    "angular-animate": "^1.5.5",
    "angular-messages": "^1.5.5",
    "angular-moment": "^1.0.0-beta.6",
    "angular-resource": "^1.5.5",
    "angular-route": "^1.5.5",
    "angular-sanitize": "^1.5.5",
    "angular-touch": "^1.5.5",
    "angularjs-viewhead": "0.0.1",
    "angulartics": "^1.0.3",
    "angulartics-google-analytics": "^0.1.4",
    "angulartics-google-tag-manager": "0.0.1",
    "async": "^1.5.2",
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "body-parser": "^1.15.1",
    "browserify": "^13.0.1",
    "bundle-collapser": "^1.2.1",
    "coinbase-service": "^1.0.3",
    "envify": "^3.4.0",
    "express": "^4.13.4",
    "konfig": "^0.2.1",
    "mime": "^1.3.4",
    "mine": "^0.1.0",
    "moment": "^2.13.0",
    "mongoose": "^4.4.19",
    "mongoose-intl-phone-number": "^0.4.3",
    "ng-dialog": "^0.6.1",
    "ngsticky": "^1.7.12",
    "node-sass": "^3.7.0",
    "paypal-rest-sdk": "^1.6.8",
    "pug": "^2.0.0-alpha8",
    "uglifyify": "^3.0.1",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  }
}

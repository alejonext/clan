{
  "name": "clan",
  "version": "0.1.71",
  "description": "Servidor de Mochileros",
  "main": "lib/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack > build.log",
    "start": "node dist",
    "postinstall": "npm cache clean",
    "dev": "nodemon --ignore 'public/*.js' -e js,pug --exec 'babel-node src'"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-member-expression-literals",
      "transform-export-extensions",
      "transform-merge-sibling-variables",
      "transform-node-env-inline"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/alejonext/clan.git"
  },
  "keywords": [
    "server",
    "api",
    "clan",
    "digital"
  ],
  "author": "AlejoNext <alejonext@gmail.com> (http://alejonext.co/)",
  "license": "GPL-2.0",
  "bugs": {
    "url": "https://github.com/alejonext/clan/issues"
  },
  "homepage": "https://github.com/alejonext/clan#README",
  "dependencies": {
    "@angular/common": "2.0.0-rc.4",
    "@angular/compiler": "2.0.0-rc.4",
    "@angular/core": "2.0.0-rc.4",
    "@angular/http": "2.0.0-rc.4",
    "@angular/platform-browser": "2.0.0-rc.4",
    "@angular/platform-browser-dynamic": "2.0.0-rc.4",
    "@angular/platform-server": "2.0.0-rc.4",
    "@angular/router": "3.0.0-beta.2",
    "angular2-universal": "~0.104.5",
    "async": "^2.0.1",
    "body-parser": "^1.15.2",
    "dropbox": "^2.1.0",
    "express": "^4.14.0",
    "ical": "^0.5.0",
    "install": "^0.8.1",
    "konfig": "^0.2.1",
    "methods": "~1.1.2",
    "moment": "^2.14.1",
    "node-sass": "^3.8.0",
    "parse5": "^1.5.0",
    "preboot": "^2.1.2",
    "pug": "^2.0.0-beta5",
    "rxjs": "5.0.0-beta.6",
    "underscore.string": "^3.3.4",
    "vhost": "^3.0.2"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.29",
    "@types/compression": "0.0.29",
